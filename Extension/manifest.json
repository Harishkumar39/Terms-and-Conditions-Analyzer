{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://localhost:5000/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
